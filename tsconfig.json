{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "bundler",
    "strict": true,
    "outDir": "./Pockitt/wwwroot/js",
    "rootDir": "./Pockitt/wwwroot/js",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["Pockitt/wwwroot/js/**/*.ts"],
  "exclude": ["node_modules"]
}